    <div class="container">
      <div class="header">
        <img src="/public/img/logo.png" alt="" class="logo">
        <h1 class="title">Atsumeru</h1>
      </div>
      <div class="feed">
        <% feed.forEach(function(item){ %>
        <div class="feed-item">
          <img src="<%- item.detail.imageUrl %> " alt="" class="feed-image">
          <div class="feed-content">
            <div class="">
              <p class="feed-date"><%-item.formattedDate %></p>
              <p class="feed-title"><%-item.title %></p>
            </div>
            <div class="feed-content-bottom">
              <a href="<%- item.detail.siteUrl %>" target="_blank" class="feed-anime-title"><%- item.animeTitle %></a>
              <svg class="feed-item-download-button" onclick=<%= item.onClick %>>
                <use href="#magnet-link" />
              </svg>
            </div>
          </div>
        </div>
        <% }); %>
      </div>
    </div>
    <script src="/public/js/main.js"></script>
